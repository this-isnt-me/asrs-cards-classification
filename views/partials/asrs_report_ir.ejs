<div class="row">
	<div class="col-3"></div>
	<div class="col-6"><hr class="style3"></div>
	<div class="col-3"></div>
 	<% for (let i = 0; i < returnDocu.acnArray.length;  i++ ) { %>
		<div class="col-6">
			<div class="mar5BR">
				<h6 class="text-left"><strong> Report Ref: </strong></h6>
				<span class="text-left mar5BR"><%= returnDocu.acnArray[i] %></span>
			</div>
		</div>
		<div class="col-6">
			<div class="mar5BR">
				<h6 class="text-left"><strong> Similarity Score: </strong></h6>
				<span class="text-left mar5BR"><%= returnDocu.scoreArray[i] %></span>
			</div>
		</div>
		<div class="col-12">
			<div class="mar5BR">
				<h6 class="text-left"><strong> Narrative: </strong></h6>
				<span class="text-left mar5BR"><%= returnDocu.narrativeArray[i] %></span>
			</div>
		</div>
	<div class="col-3"></div>
	<div class="col-6"><hr class="style3"></div>
	<div class="col-3"></div>
	<% } %>
</div>
<hr class="style3">