<div id="accessSection">
	<div class="row">
		<div class="col-sm-1"></div>
		<div class="col-sm-10">
			<h2 class="navunder text-center"><strong>Upload CSV</strong></h2>
			<hr class="style2">	
			<div id="fileUploadSection">
				<div class="row">
					<div class="col-1"></div>
					<div class="col-10">
						<p class="fontSize175">Select file to Upload</p>
						<input type="file" id="csvUpload" name="csvUpload" class="modalForm" accept=".csv" required>
					</div>
					<div class="col-1"></div>
				</div>
				<div class="row">
					<div class="col-md-4"></div>
					<div class="col-12 col-md-4">
						<button type="button" id="csvUploadBtn" name="csvUploadBtn" class="modalForm btn btn-outline-light">Upload Cards</button>
					</div>
					<div class="col-md-4"></div>
				</div>
			</div>
			<br>
			<div class="row">
				<div class="col-12">
					<div id="msg" class="text-center"></div>
				</div>
			</div>
			<hr class="style2">
		</div>
		<div class="col-sm-1"></div>
	</div>
</div>